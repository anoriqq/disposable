{
  "name": "disposable",
  "license": "MIT",
  "author": "anoriqq <shota.yoshikawa@anoriqq.com>",
  "repository": "git@github.com:anoriqq/disposable.git",
  "version": "1.0.0",
  "scripts": {
    "build": "yarn clean && next build && yarn build:server",
    "build:server": "tsc --project ./server/tsconfig.json",
    "clean": "rimraf ./dist ./.next",
    "deploy": "gcloud app deploy",
    "dev": "nodemon",
    "dev:inspect": "nodemon --inspect",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\"",
    "gcp-build": "yarn build",
    "kill-port:node": "kill-port ${PORT:-8000} ${PORT:-9229}",
    "lint": "eslint --fix ./server ./pages",
    "oax": "oax generate ./openapi.yaml -d ./server/@types/express",
    "oax:watch": "yarn oax -w",
    "start": "cross-env NODE_ENV='production' node ./dist/index.js",
    "test": "jest",
    "test:ci": "jest --ci --coverage",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@google-cloud/compute": "1.2.0",
    "@google-cloud/connect-firestore": "1.0.6",
    "@google-cloud/firestore": "3.8.0",
    "@material-ui/core": "4.9.14",
    "@material-ui/lab": "4.0.0-alpha.53",
    "@zeit/next-css": "1.0.1",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.5",
    "cross-env": "7.0.2",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "gce-images": "2.1.4",
    "googleapis": "51.0.0",
    "helmet": "3.22.0",
    "isomorphic-unfetch": "3.0.0",
    "mongoose": "5.9.13",
    "next": "9.4.0",
    "passport": "0.4.1",
    "passport-google-oauth20": "2.0.0",
    "prop-types": "15.7.2",
    "qs": "6.9.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "5.7.2",
    "swr": "0.2.0",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.9.0",
    "@testing-library/jest-dom": "5.7.0",
    "@testing-library/react": "10.0.4",
    "@types/cookie-parser": "1.4.2",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.6",
    "@types/express-session": "1.17.0",
    "@types/helmet": "0.0.47",
    "@types/jest": "25.2.2",
    "@types/mongoose": "5.7.16",
    "@types/node": "13.13.6",
    "@types/passport": "1.0.3",
    "@types/passport-google-oauth20": "2.0.3",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "babel-jest": "26.0.1",
    "eslint": "7.0.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.11.0",
    "eslint-plugin-jest-dom": "2.1.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-testing-library": "3.1.2",
    "eslint-plugin-tsc": "1.2.0",
    "husky": "4.2.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.0.1",
    "kill-port": "1.6.0",
    "lint-staged": "10.2.2",
    "nodemon": "2.0.3",
    "prettier": "2.0.5",
    "prettier-package-json": "2.1.3",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "ts-jest": "25.5.1",
    "ts-node": "8.10.1",
    "typed-oax": "0.0.4",
    "typescript": "3.8.3",
    "url-loader": "4.1.0"
  },
  "engines": {
    "node": "12.x.x",
    "yarn": "^1.22.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "prettier-package-json --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --plugin tsc --rule 'tsc/config: [2, {configFile: \"tsconfig.json\"}]'",
      "cross-env NODE_ENV=test jest --bail --findRelatedTests"
    ],
    "*.{json,md,html,css}": [
      "prettier --write"
    ]
  }
}
