{
  "name": "disposable",
  "license": "MIT",
  "author": "anoriqq <shota.yoshikawa@anoriqq.com>",
  "repository": "git@github.com:anoriqq/disposable.git",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "next build && tsc --project ./server/tsconfig.json",
    "dev": "nodemon",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\"",
    "lint": "eslint --fix ./server ./pages",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "jest --colors",
    "test:ci": "jest --ci --coverage",
    "test:coverage": "jest --coverage --colors",
    "type-check": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.5",
    "cross-env": "7.0.2",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "isomorphic-unfetch": "3.0.0",
    "mongoose": "5.9.13",
    "next": "9.3.6",
    "passport": "0.4.1",
    "passport-google-oauth20": "2.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "swr": "0.2.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.9.0",
    "@testing-library/jest-dom": "5.7.0",
    "@testing-library/react": "10.0.4",
    "@types/cookie-parser": "1.4.2",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.6",
    "@types/express-session": "1.17.0",
    "@types/jest": "25.2.1",
    "@types/mongoose": "5.7.16",
    "@types/node": "13.13.5",
    "@types/passport": "1.0.3",
    "@types/passport-google-oauth20": "2.0.3",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "babel-jest": "26.0.1",
    "eslint": "7.0.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.10.0",
    "eslint-plugin-jest-dom": "2.1.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-testing-library": "3.1.2",
    "husky": "4.2.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.0.1",
    "lint-staged": "10.2.2",
    "nodemon": "2.0.3",
    "prettier": "2.0.5",
    "prettier-package-json": "2.1.3",
    "react-test-renderer": "16.13.1",
    "ts-jest": "25.5.1",
    "ts-node": "8.10.1",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "prettier-package-json --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{ts,tsx}": [
      "yarn type-check"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
